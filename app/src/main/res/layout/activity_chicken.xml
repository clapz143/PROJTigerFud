<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/imageRecyclerView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ChickenActivity">

  <TextView
      android:id="@+id/textTitle2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:textColor="@color/brown"
      app:fontFamily="@font/bezier"
      android:text="Chicken"
      android:textSize="30sp"
      tools:ignore="MissingConstraints"
      tools:layout_editor_absoluteX="31dp"
      tools:layout_editor_absoluteY="16dp" />

  <androidx.recyclerview.widget.RecyclerView
      android:id="@+id/_imageRecyclerView2"
      android:layout_width="374dp"
      android:layout_height="600dp"
      android:layout_margin="16dp"
      android:layout_marginEnd="16dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textTitle2" />

  <Button
      android:id="@+id/backbutton"
      android:layout_width="107dp"
      android:layout_height="54dp"
      android:layout_marginBottom="33dp"
      android:backgroundTint="@color/brown"
      android:gravity="center"
      android:onClick="backBtnClick"
      android:textSize="100dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/_imageRecyclerView2"
      tools:ignore="MissingConstraints" />

  <TextView
      android:id="@+id/text0"
      android:layout_width="65dp"
      android:layout_height="131dp"
      android:layout_marginEnd="15dp"
      android:layout_marginBottom="50dp"
      android:text="â¬… "
      android:textColor="@color/white"
      android:textSize="80dp"
      android:textStyle="bold"
      app:fontFamily="@font/bezier"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/_imageRecyclerView2"
      tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>